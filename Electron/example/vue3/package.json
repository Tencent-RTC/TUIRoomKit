    "axios": "^0.27.2",
    "interactjs": "^1.10.26",
    "js-cookie": "^3.0.1",
    "lodash.isequal": "^4.5.0",
    "mitt": "^3.0.0",
    "pinia": "2.0.24",
    "sass": "^1.56.1",
    
    "vue-i18n": "^9.12.1",
    "vue-router": "^4.0.14",
    "yuv-buffer": "^1.0.0",
    "yuv-canvas": "^1.2.11"
