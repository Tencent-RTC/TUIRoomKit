{
  "name": "room-uikit-electron-vue3",
  "version": "2.3.3",
  "description": "This is an interactive multimedia room solution that published by Tencent Cloud RTC team for PC base on Electron, vue and so on.",
  "main": "dist/main/index.cjs",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "rm -rf node_modules/.vite && cross-env VITE_RUNTIME_SCENE=inner node scripts/watch.mjs",
    "prebuild": "vue-tsc --noEmit --p packages/renderer/tsconfig.json && node scripts/build.mjs",
    "build:inner-win": "cross-env VITE_RUNTIME_SCENE=inner node scripts/build.mjs && electron-builder --win",
    "build:inner": "cross-env VITE_RUNTIME_SCENE=inner node scripts/build.mjs && electron-builder",
    "build:outer": "cross-env VITE_RUNTIME_SCENE=outer VITE_RUNTIME_ENV=official node scripts/build.mjs && electron-builder",
    "build:mac-inner-universal": "cross-env VITE_RUNTIME_SCENE=inner node scripts/build.mjs && electron-builder build --mac --universal",
    "build:mac-outer-universal": "cross-env VITE_RUNTIME_SCENE=outer VITE_RUNTIME_ENV=official node scripts/build.mjs && electron-builder build --mac --universal",
    "build": "electron-builder",
    "init": "git config core.hooksPath .git/hooks/ && rm -rf .git/hooks && npx simple-git-hooks",
    "test:e2e": "npx playwright test",
    "test:e2e:headless": "npx playwright test --headed",
    "copy:outside": "cp -R ../electron-tui-room/packages ../../../electron-tui-room/",
    "copy:tuiroom": "node scripts/copy-tuiroom.js",
    "publish-github": "node scripts/publish.js && node scripts/generate-external-package-json.js",
    "replace": "node scripts/replace.js",
    "postinstall-mac:x64": "rsync -a ./node_modules/trtc-electron-sdk/build/mac-framework/x64/ ./node_modules/electron/dist/Electron.app/Contents/Frameworks",
    "postinstall-mac:arm64": "rsync -a ./node_modules/trtc-electron-sdk/build/mac-framework/arm64/ ./node_modules/electron/dist/Electron.app/Contents/Frameworks",
    "postinstall": "node scripts/postinstall.js"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@vitejs/plugin-vue": "^2.3.2",
    "cross-env": "^7.0.3",
    "electron": "19.0.1",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.0.0",
    "jose": "^1.28.0",
    "nano-staged": "^0.8.0",
    "replace": "^1.2.1",
    "request": "^2.88.2",
    "simple-git-hooks": "^2.8.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.8",
    "vite-plugin-electron": "^0.4.5",
    "vite-plugin-resolve": "^2.1.2",
    "vue": "~3.3.13",
    "vue-tsc": "^0.35.2"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "dependencies": {
    "@tencentcloud/chat": "latest",
    "@tencentcloud/roomkit-electron-vue3": "^2.3.3",
    "@tencentcloud/tui-core": "latest",
    "@tencentcloud/tuiroom-engine-electron": "^2.3.3",
    "@tencentcloud/universal-api": "^2.0.9",
    "axios": "^0.27.2",
    "interactjs": "^1.10.26",
    "js-cookie": "^3.0.1",
    "lodash.isequal": "^4.5.0",
    "mitt": "^3.0.0",
    "pinia": "^2.0.24",
    "sass": "^1.56.1",
    "trtc-electron-sdk": "11.7.603",
    "vue-i18n": "^9.12.1",
    "vue-router": "^4.0.14",
    "yuv-buffer": "^1.0.0",
    "yuv-canvas": "^1.2.11"
  },
  "repository": "https://git.woa.com:TRDCWeb/TUIRoomEngine.git"
}