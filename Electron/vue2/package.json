{
  "name": "room-uikit-electron-vue2",
  "version": "1.0.2",
  "main": "main.electron.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "concurrently \"npm run serve\" \"npm run electron \"",
    "build:mac": "vue-cli-service build --trtc_env=production --target_platform=darwin",
    "build:win": "vue-cli-service build --trtc_env=production --target_platform=win32",
    "compile:mac": "node_modules/.bin/electron-builder --mac",
    "compile:win64": "node_modules/.bin/electron-builder --win --x64",
    "pack:mac": "npm run build:mac && npm run compile:mac",
    "pack:win64": "npm run build:win && npm run compile:win64",
    "electron": "electron . --trtc_env=development"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "electron": "^18.3.12",
    "electron-builder": "^23.6.0",
    "eslint": "^7.32.0",
    "eslint-config-tencent": "^1.0.4",
    "eslint-plugin-vue": "^8.0.3",
    "native-ext-loader": "^2.3.0",
    "sass-loader": "^13.2.0",
    "typescript": "~4.5.5",
    "vue-template-compiler": "^2.6.14",
    "string-replace-webpack-plugin": "^0.1.3",
    "concurrently": "^5.2.0"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "dependencies": {
    "@tencentcloud/tuiroom-engine-electron": "1.0.2",
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "element-ui": "^2.15.10",
    "js-cookie": "^3.0.1",
    "lodash.isequal": "^4.5.0",
    "pinia": "^2.0.28",
    "sass": "^1.57.1",
    "tim-js-sdk": "^2.24.2",
    "trtc-electron-sdk": "10.3.405",
    "tsignaling": "^0.10.0",
    "vue": "^2.7.14",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.28.2",
    "vue-i18n-bridge": "^9.2.2",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.6.5",
    "yuv-buffer": "^1.0.0",
    "yuv-canvas": "^1.2.11"
  }
}
